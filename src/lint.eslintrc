{
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "max-lines-per-function": [
      "error",
      30
    ],
    "semi": [
      "error",
      "never"
    ],
  }
}